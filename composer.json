{
    "name": "hypernode/deploy",
    "description": "Hypernode Deploy",
    "type": "application",
    "bin": [
        "bin/hipex-deploy.php",
        "bin/hipex-deploy"
    ],
    "require": {
        "ext-json": "*",
        "ext-zlib": "*",
        "deployer/deployer": "^6.8",
        "deployer/recipes": "dev-master",
        "doctrine/annotations": "^1.6",
        "hypernode/deploy-configuration": "dev-master",
        "ocramius/package-versions": "^1.10",
        "ocramius/proxy-manager": "^2.1",
        "php-di/php-di": "^6.0",
        "psr/log": "^1.0",
        "sentry/sdk": "^2.0",
        "symfony/console": "^4.1",
        "symfony/finder": "^4.1",
        "symfony/http-client": "^4.4",
        "twig/twig": "^2.12"
    },
    "autoload": {
        "psr-4": {
            "HipexDeploy\\": "src/HipexDeploy/"
        },
        "files": [
            "src/HipexDeploy/Deployer/functions.php"
        ]
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5",
        "roave/security-advisories": "dev-master"
    },
    "config": {
        "platform": {
            "php": "7.2"
        },
        "preferred-install": {
            "hypernode/*": "source"
        },
        "allow-plugins": {
            "ocramius/package-versions": true
        },
        "sort-packages": true
    },
    "repositories": {
        "hypernode-deploy-configuration": {
            "type": "git",
            "url": "git@github.com:ByteInternet/hypernode-deploy-configuration.git"
        }
    }
}
