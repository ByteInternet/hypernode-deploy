{
    "name": "hypernode/deploy",
    "description": "Hypernode Deploy",
    "type": "application",
    "license": "proprietary",
    "bin": [
        "bin/hypernode-deploy.php",
        "bin/hypernode-deploy"
    ],
    "require": {
        "ext-json": "*",
        "ext-zlib": "*",
        "composer-runtime-api": "^2",
        "deployer/deployer": "dev-master#ff3c33c as v7.0.0",
        "doctrine/annotations": "^1.6",
        "hypernode/deploy-configuration": "2.0.0-alpha3",
        "php-di/php-di": "^6.0",
        "psr/log": "^1.0",
        "symfony/console": "^5.4",
        "symfony/finder": "^5.4",
        "symfony/http-client": "^5.4",
        "symfony/process": "^5.4",
        "twig/twig": "^2.12",
        "webmozart/assert": "^1.11"
    },
    "autoload": {
        "psr-4": {
            "Hypernode\\Deploy\\": "src/"
        },
        "files": [
            "src/Deployer/functions.php"
        ]
    },
    "require-dev": {
        "phpro/grumphp-shim": "^1.13",
        "phpunit/phpunit": "^8.5",
        "roave/security-advisories": "dev-master",
        "squizlabs/php_codesniffer": "^3.7",
        "vimeo/psalm": "^4.26"
    },
    "config": {
        "preferred-install": {
            "hypernode/*": "source"
        },
        "sort-packages": true,
        "allow-plugins": {
            "phpro/grumphp-shim": true
        }
    }
}
