{
    "name": "hipex/deploy",
    "description": "Hipex deploy image",
    "type": "application",
    "require-dev": {
        "roave/security-advisories": "dev-master"
    },
    "bin": [
        "bin/hipex-deploy.php",
        "bin/hipex-deploy"
    ],
    "config": {
        "platform": {
            "php": "7.2"
        },
        "preferred-install": {
            "hipex/*": "source"
        }
    },
    "autoload": {
        "psr-4": {
            "HipexDeploy\\": "src/HipexDeploy/"
        },
        "files": [
            "src/HipexDeploy/Deployer/functions.php"
        ]
    },
    "require": {
        "ext-zlib": "*",
        "ext-json": "*",
        "deployer/deployer": "^6.8",
        "symfony/console": "^4.1",
        "symfony/finder": "^4.1",
        "php-di/php-di": "^6.0",
        "doctrine/annotations": "^1.6",
        "ocramius/proxy-manager": "^2.1",
        "ocramius/package-versions": "^1.10",
        "psr/log": "^1.0",
        "sentry/sdk": "^2.0",
        "twig/twig": "^2.12",
        "deployer/recipes": "dev-master",
        "hypernode/deploy-configuration": "dev-master"
    },
    "repositories": {
        "hypernode-deploy-configuration": {
            "type": "git",
            "url": "git@github.com:ByteInternet/hypernode-deploy-configuration.git"
        }
    }
}
